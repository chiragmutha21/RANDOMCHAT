{
  "version": 2,
  "routes": [
    {
      "src": "/Style/(.*)",
      "dest": "/frontend/Style/$1"
    },
    {
      "src": "/script/(.*)",
      "dest": "/frontend/script/$1"
    },
    {
      "src": "/group-selection.html",
      "dest": "/frontend/components/group-selection.html"
    },
    {
      "src": "/index.html",
      "dest": "/frontend/components/index.html"
    },
    {
      "src": "/loginpage.html",
      "dest": "/frontend/components/loginpage.html"
    },
    {
      "src": "/",
      "dest": "/frontend/components/loginpage.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/components/loginpage.html"
    }
  ]
}